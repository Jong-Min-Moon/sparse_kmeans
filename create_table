DROP TABLE IF EXISTS sparse_kmeans_isee
CREATE TABLE IF NOT EXISTS sparse_kmeans_isee
(
    rep INTEGER,
    iter INTEGER,
    sep REAL,
    dim INTEGER,
    rho REAL,
    sparsity INTEGER,
    acc REAL,
    discov_true INTEGER,
    discov_false INTEGER,
    diff_x_tilde REAL,
    diff_omega_diag REAL,
    time_isee TEXT, -- interval types are not directly supported in SQLite
    time_sdp TEXT, -- interval types are not directly supported in SQLite
    jobdate TIMESTAMP
);